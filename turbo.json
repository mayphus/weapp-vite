{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "test": {
      "dependsOn": [
        "^build"
      ]
    },
    "release": {},
    "sync": {},
    "website#build": {
      "dependsOn": [
        "build" // 自己的构建任务
      ]
    }
  }

}
