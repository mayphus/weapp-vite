{
  "name": "rolldown-bundle-require",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "esbuild build package template",
  "author": "ice breaker <1324318532@qq.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonofmagic/monorepo-template.git",
    "directory": "packages/unbuild-template"
  },
  "bugs": {
    "url": "https://github.com/sonofmagic/monorepo-template/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "dev": "unbuild --stub",
    "build:watch": "tsup --watch --sourcemap",
    "build": "tsup",
    "test": "vitest run",
    "test:dev": "vitest",
    "release": "pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "publishConfig": {},
  "peerDependencies": {
    "esbuild": ">=0.18",
    "rolldown": ">=1.0.0-beta.13"
  },
  "dependencies": {
    "get-tsconfig": "^4.10.1"
  }
}
